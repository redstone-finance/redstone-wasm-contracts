{
  "name": "redstone-smartcontracts-assemblyscript-tmpl",
  "version": "0.0.1",
  "description": "RedStone SmartWeave wasm assemblyscript contract template",
  "main": "index.js",
  "author": "Redstone Team <dev@redstone.finance>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "asbuild:fake-news": "asc assemblyscript/fake-news/assembly/contract.ts --runtime stub --exportRuntime --transform ./ContractTransform --exportTable --target release",
    "deploy:fake-news:testnet": "yarn asbuild && node deploy/testnet/deploy-test.js",
    "deploy:fake-news:mainnet": "yarn asbuild && node deploy/mainnet/deploy-mainnet.js",
    "test:fake-news": "yarn asbuild:fake-news && yarn jest ./tests/fake-news-contract.spec.ts",
    "test": "yarn test:fake-news"
  },
  "engines": {
    "node": ">=16.5"
  },
  "dependencies": {
    "@serial-as/json": "^1.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@assemblyscript/loader": "0.19.23",
    "@types/jest": "^27.4.1",
    "arlocal": "1.1.30",
    "arweave": "^1.10.23",
    "assemblyscript": "^0.19.23",
    "asyncify-wasm": "^1.2.1",
    "jest": "^27.5.1",
    "redstone-smartweave": "0.4.53",
    "ts-jest": "^27.1.3",
    "visitor-as": "^0.8.0"
  }
}
